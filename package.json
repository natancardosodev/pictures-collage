        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-import": "^2.21.2",
        "eslint-plugin-prettier": "^5.0.0",
        "prettier": "^3.0.1",
        "prettier-eslint": "^15.0.1",
        "stylelint": "^13.13.1",
        "stylelint-config-sass-guidelines": "^9.0.1"
