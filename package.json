{
    "name": "pictures-collage",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "author": "Natan Cardoso",
    "scripts": {
        "prepare": "husky install",
        "stylelint": "./node_modules/.bin/stylelint 'assets/css/*.scss'",
        "stylelint:fix": "./node_modules/.bin/stylelint 'assets/css/*.scss' --fix",
        "eslint": "./node_modules/.bin/eslint 'assets/js/*.js' --quiet",
        "eslint:fix": "./node_modules/.bin/eslint 'assets/js/*.js' --fix",
        "lint": "npm run stylelint",
        "lint:fix": "npm run stylelint:fix",
        "rm-dist": "rm -rf public/*",
        "hash": "node import-generator/hash.js",
        "check-version": "./node_modules/.bin/check-node-version --node 14.19.1 --npm 6.14.16",
        "start": "npm run imports && npm run watch",
        "imports": "npm run rm-dist",
        "watch": "webpack-dashboard -- webpack --config webpack.dev.js --watch",
        "prod": "webpack --config webpack.prod.js",
        "rm": "rm -rf public/images , public/node_modules",
        "build": "npm run imports && npm run prod && npm run rm"
    },
    "license": "ISC",
    "engines": {
        "node": ">=14.19.1"
    },
    "dependencies": {
        "@babel/core": "^7.10.2",
        "@babel/preset-env": "^7.10.2",
        "@fortawesome/fontawesome-free": "^6.4.2",
